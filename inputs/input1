from requests import get
import py_compile

def getcode(url, getcode = None): #getcode 코드파일 가져올 변수명
    if not getcode:
       getcode = url.split('/')[-1]
       
    with open(getcode, 'wb') as file:
       response = get(url)
       file.write(response.content)
       
